{
  "*.{js,jsx,ts,tsx}": [
    "eslint --fix",
    "prettier --write"
  ],
  "*.{json,md,yml,yaml}": [
    "prettier --write"
  ],
  "package.json": [
    "prettier --write"
  ],
  "src/**/*.{ts,tsx}": [
    "tsc --noEmit --skipLibCheck"
  ],
  "scripts/**/*": [
    "eslint --fix",
    "prettier --write"
  ]
}